{
  "$schema": "https://raw.githubusercontent.com/sourcegraph/sourcegraph/master/shared/src/schema/extension.schema.json",
  "name": "@sourcegraph/lsif-typescript",
  "version": "0.1.10",
  "description": "LSIF indexer for TypeScript and JavaScript",
  "publisher": "sourcegraph",
  "bin": "dist/src/main.js",
  "main": "./dist/src/main.js",
  "scripts": {
    "prettier": "prettier --write --list-different '**/*.{ts,js?(on),md,yml}'",
    "prettier-check": "npm run prettier --write=false",
    "tslint": "tslint -p tsconfig.json --format stylish",
    "eslint": "eslint --cache '**/*.ts?(x)'",
    "build": "node ./node_modules/typescript/bin/tsc -b .",
    "test": "uvu -r ts-node/register --ignore dist",
    "update-snapshots": "uvu -r ts-node/register --ignore dist --update-snapshots",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sourcegraph/lsif-typescript.git"
  },
  "keywords": [
    "lsif",
    "typescript",
    "compiler",
    "javascript",
    "indexer"
  ],
  "author": "Code Intelligence at Sourcegraph",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sourcegraph/lsif-typescript/issues"
  },
  "homepage": "https://github.com/sourcegraph/lsif-typescript#readme",
  "dependencies": {
    "google-protobuf": "^3.19.3",
    "typescript": "^4.5.4",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@sourcegraph/eslint-config": "0.27.0",
    "@sourcegraph/prettierrc": "3.0.3",
    "@sourcegraph/tsconfig": "4.0.1",
    "@types/diff": "5.0.2",
    "@types/google-protobuf": "3.15.5",
    "@types/node": "17.0.14",
    "@types/yargs": "17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "diff": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-unicorn": "^21.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "2.5.1",
    "ts-node": "^10.4.0",
    "typescript-eslint": "0.0.1-alpha.0",
    "uvu": "^0.5.3"
  }
}
